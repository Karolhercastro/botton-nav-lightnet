<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Base application theme. -->
    <style name="Theme.bottomnavigationkotlin" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_300</item>
        <item name="colorPrimaryVariant">@color/purple_500</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/green_300</item>
        <item name="colorSecondaryVariant">@color/green_500</item>
        <item name="colorOnSecondary">@color/green_700</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" targetApi="l">?attr/colorOnPrimary</item>
        <!-- Status bar icon color -->
        <item name="android:windowLightStatusBar">true</item>
    </style>
</resources>